$ORIGIN isc.org.
@       IN SOA (
                host-dns.isc.org.   ; MNAME
                admin.isc.org.      ; RNAME
                2024041501          ; serial
                3600                ; refresh (1 hour)
                900                 ; retry (15 minutes)
                604800              ; expire (1 week)
                86400               ; minimum (1 day)
                )

i2-2-8        IN A    2.8.0.2
i2-3-8        IN A    3.8.0.2
i2-100        IN A    100.0.1.2

rootdns-i IN A    100.0.1.100
