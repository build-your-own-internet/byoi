$ORIGIN google.com.
@       IN SOA (
                host-dns.google.com.    ; MNAME
                admin.google.com        ; RNAME
                2024041501              ; serial
                3600                    ; refresh (1 hour)
                900                     ; retry (15 minutes)
                604800                  ; expire (1 week)
                86400                   ; minimum (1 day)
                )

server-g1       IN A    8.2.0.11
server-g2       IN A    8.2.0.12
server-g3       IN A    8.2.0.13

resolver-g      IN A    8.1.0.100

tlddns-g        IN A    8.2.0.100

g2-8-1          IN A    8.1.0.2
g2-8-2          IN A    8.2.0.2

g3-8-2          IN A    8.2.0.3
g3-2-8          IN A    2.8.0.3

g4-8-1          IN A    8.1.0.4
g4-3-8          IN A    3.8.0.4
