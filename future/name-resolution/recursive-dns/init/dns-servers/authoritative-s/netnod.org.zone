@       IN SOA (
                host-dns.netnod.org.   ; MNAME
                admin.netnod.org.      ; RNAME
                2024041501          ; serial
                3600                ; refresh (1 hour)
                900                 ; retry (15 minutes)
                604800              ; expire (1 week)
                86400               ; minimum (1 day)
                )

n2-2-4        IN A    2.4.0.2
n2-3-4        IN A    3.4.0.2
n2-101        IN A    101.0.1.2

tlddns-n      IN A    101.0.1.101
rootdns-n     IN A    101.0.1.100
